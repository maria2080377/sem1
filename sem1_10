#include <iostream>      //Задача 10
using namespace std;

int main() {
    setlocale(LC_ALL, "RU");
    int a;

    cout << "Введите число: ";
    cin >> a;

    if (a < 0) {
        cout << "Число отрицательное";
    }
    else if (a > 0) {
        cout << "Число положительное";
    }
    else {
        cout << "Число равно 0";
    }

    return 0;
}
